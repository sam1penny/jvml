val rec quick = fun ts -> fun sorted ->
    match ts with
        | [] -> sorted
        | [x]  -> x :: sorted
        | (x :: xs) -> (
            let rec part = fun l -> fun r -> fun u ->
                match u with
                    | [] -> quick l (x :: (quick r sorted))
                    | (y::ys) -> (
                        if ((y < x) || (y = x)) then
                            part (y::l) r ys
                        else
                            part l (y::r) ys
                    )
            in
            part [] [] xs
        )

val quicksort = fun l -> quick l []

